n=int(input())
for I in range (n):
    l=[]
    p=1
    for I in range (4):
        m=list (map(int, input(). split()))
        l. append (m)
    l. sort()    
    for j in range (2):
        if l[0][0]!=l[2][0]:
            p*=abs(l[0][0]-l[2][0])
        elif l[0][1] !=l[2][1]:
            p*=abs(l[0][1]-l[+2][1])  
    print (p)       