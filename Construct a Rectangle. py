n=int(input())
for I in range (n):
    m=list(map(int, input(). split()))
    m. sort()
    if m.count(m[0])>1:
        if m[2]%2==0:
            print ('YES')
        else :
            print ('NO')  
    elif m.count(m[2])>1:
        if m[0]%2==0:
            print ('YES')
        else :
            print ('NO')    
    else :
        if m[2]==m[1]+m[0]:
            print ('YES')         
        else :
            print ('NO')  